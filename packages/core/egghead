set mod-path			"modules/"
set help-path   		"help/"
set use-ssl     		1
set net-type   			Other

loadmodule     			channels
loadmodule     			server
loadmodule     			ctcp
loadmodule     			irc
loadmodule     			notes
loadmodule     			console
#loadmodule    			blowfish
loadmodule     			pbkdf2
loadmodule     			transfer
loadmodule     			share

bind evnt - init-server evnt:init-server 

proc evnt:init-server {type} {
    global botnick
    putquick "MODE $botnick +i-ws"
    #putquick "PRIVMSG NickServ :identify password"
}

server add servername

